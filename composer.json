{
    "name": "artisanry/twitter",
    "description": "A Twitter bridge for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Twitter",
        "Laravel Twitter"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0",
        "abraham/twitteroauth": "^1.0.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Twitter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Twitter\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Twitter\\TwitterServiceProvider"
            ],
            "aliases": {
                "Twitter": "Artisanry\\Twitter\\Facades\\Twitter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
