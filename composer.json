{
    "name": "faustbrian/laravel-twitter",
    "description": "A Twitter bridge for Laravel",
    "keywords": ["laravel", "framework", "Laravel-Twitter", "Laravel Twitter", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.5.* ||Â 5.6.*",
        "abraham/twitteroauth": "^0.7.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Twitter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Twitter\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Twitter\\TwitterServiceProvider"
            ],
            "aliases": {
                "Twitter": "BrianFaust\\Twitter\\Facades\\Twitter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
